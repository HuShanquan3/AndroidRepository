<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ytempest.animationset">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".traditional.frame.FrameActivity" />
        <activity android:name=".traditional.tween.TweenActivity" />
        <activity android:name=".interpolator.InterpolatorActivity" />
        <activity android:name=".interpolator.ShowInterpolatorActivity" />
        <activity android:name=".evaluator.EvaluatorActivity" />
        <activity android:name=".property.IntValueActivity" />
        <activity android:name=".property.PropertyActivity"></activity>
    </application>

</manifest>