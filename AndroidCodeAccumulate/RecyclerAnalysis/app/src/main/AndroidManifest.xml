<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ytempest.recycleranalysis">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".divisionUse.DivisionUseActivity" />
        <activity android:name=".commonRecyclerUse.CommonRecyclerActivity" />
        <activity android:name=".headerAndFooter.HeaderFooterActivity" />
        <activity android:name=".loadRefresh.RefreshLoadActivity" />
        <activity android:name=".itemDragUse.DragItemActivity" />
        <activity android:name=".ListIndicator.ListIndicatorActivity"></activity>
    </application>

</manifest>